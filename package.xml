<?xml version="1.0" encoding="ISO-8859-1" ?>
<!-- do not use the "Type" attribute here, that one is only for
     generated package.xml files -->
<!DOCTYPE package SYSTEM "../package.dtd">
<package version="1.0">
  <name>SOAP</name>
  <summary>SOAP Client/Server for PHP</summary>
  <description>
    Implementation of SOAP protocol and services
  </description>
  <maintainers>
    <maintainer>
      <user>shane</user>
      <name>Shane Caraveo</name>
      <email>shane@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <license>PHP License</license>
  <release>
    <state>beta</state>
    <version>0.6.2</version>
    <date>2002-07-06</date>
    <notes>
      This is the initial independent PEAR release.
    </notes>
    <filelist>
      <dir name="/" baseinstalldir="SOAP">
        <file role="php" name="Base.php"/>
        <file role="php" name="Client.php"/>
        <file role="php" name="Fault.php"/>
        <file role="php" name="Parser.php"/>
        <file role="php" name="Server.php"/>
        <file role="php" name="Transport.php"/>
        <file role="php" name="Value.php"/>
        <file role="php" name="WSDL.php"/>
        <dir name="example" role="doc"> 
          <file role="doc" name="stockquote.php"/>
          <file role="doc" name="server.php"/>
          <file role="doc" name="client.php"/>
          <file role="doc" name="smtp.php"/>
          <file role="doc" name="email_server.php"/>
          <file role="doc" name="email_gateway.php"/>
        </dir>
        <dir name="tools"> 
          <file role="php" name="genproxy.php"/>
        </dir>
        <dir name="Transport"> 
          <file role="php" name="HTTP.php"/>
          <file role="php" name="SMTP.php"/>
        </dir>
        <dir name="Server"> 
          <file role="php" name="Email.php"/>
          <file role="php" name="Email_Gateway.php"/>
        </dir>
        <dir name="Type"> 
          <file role="php" name="dateTime.php"/>
          <file role="php" name="duration.php"/>
          <file role="php" name="hexBinary.php"/>
        </dir>
        <dir name="interop" role="test"> 
          <file role="test" name="index.php"/>
          <file role="test" name="client_round2.php"/>
          <file role="test" name="client_round2_run.php"/>
          <file role="test" name="client_round2_interop.php"/>
          <file role="test" name="client_round2_params.php"/>
          <file role="test" name="client_round2_results.php"/>
          <file role="test" name="database_round2.sql"/>
          <file role="test" name="echoheadersvc.wsdl"/>
          <file role="test" name="interop.wsdl"/>
          <file role="test" name="interopB.wsdl"/>
          <file role="test" name="readme.txt"/>
          <file role="test" name="server_round2.php"/>
          <file role="test" name="server_round2_base.php"/>
          <file role="test" name="server_round2_groupB.php"/>
          <file role="test" name="server_round2_groupC.php"/>
          <file role="test" name="server_round2_test.php"/>
        </dir>
        <dir name="test" role="test"> 
          <file role="test" name="5.2.simpletypes.php"/>
          <file role="test" name="5.2.1.simpletypes.string.php"/>
          <file role="test" name="5.2.1.simpletypes.multiref.php"/>
          <file role="test" name="all.php"/>
          <file role="test" name="smtp.php"/>
          <file role="test" name="test.utility.php"/>
          <file role="test" name="xmethods.php"/>
        </dir>
      </dir>
    </filelist>
    <deps>
      <dep type="php" rel="ge" version="4.1"/>
      <dep type="ext" rel="has">bcmath</dep>
      <dep type="ext" rel="has">pcre</dep>
      <dep type="pkg" rel="has">Mail_Mime</dep>
      <dep type="pkg" rel="ge" version="0.1">Net_DIME</dep>
    </deps>
  </release>
</package>
